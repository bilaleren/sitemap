{
    "name": "bilaleren/sitemap",
    "description": "PHP sitemap.xml generator.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "PHP Sitemap",
        "Sitemap",
        "sitemap.xml"
    ],
    "authors": [
        {
            "name": "Bilal Eren",
            "email": "bilal@webnoi.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "SiteMap\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SiteMap\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
